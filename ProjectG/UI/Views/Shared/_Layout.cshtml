    <a class="profile-widget" href="https://normativka.by/lib/rubric/22519/sid/ba8a856ad1494cca8062389dcfe3e880#" tabindex="6">
      <div class="profile-widget__container-login">
        <i class="profile-widget__login-icon mdi mdi-lock"></i>
        <div class="profile-widget__login-text">Вход в систему</div>
      </div>
    </a>
    <div class="profile-widget__dropdown">
      <div class="auth auth_popup">
        <form class="auth__form">
          <div class="form-cell">
            <input type="text" id="email" name="email" placeholder="Электронная почта" class="auth__input-text">
            <ul class="f-error hide"></ul>
          </div>
          <div class="form-cell">
            <div class="ctrl ctrl__input-show-symbols">
              <span class="mdi mdi-eye-off"></span>
            </div>
            <input type="password" id="password" name="password" placeholder="Пароль" class="auth__input-text auth__input-pass">
            <ul class="f-error hide"></ul>
          </div>
          <div class="form-cell clearfix">
            <div class="pull-left">
              <input type="checkbox" id="auth" class="auth__remember-checkbox" value="on">
              <label for="auth" class="auth__remember-label">Запомнить меня</label>
            </div>
            <div class="pull-right">
              <a href="https://normativka.by/user/recover/" target="_blank" class="auth__recover-pass">Восстановить пароль</a>
            </div>
          </div>
          <div class="form-cell">
            <input type="submit" value="Войти" class="ctrl auth__submit">
          </div>
        </form>
        <div class="auth__registration">
          <a href="https://normativka.by/user/new/step1/" target="_blank" class="ctrl ctrl__registration"><i class="mdi mdi-plus-circle-outline"></i><!-- react-text: 96 -->Регистрация<!-- /react-text --></a>
        </div>
      </div>
    </div>
  </div>
  <div class="header__container-search">
    <div id="search-form" class="search">
      <button class="ctrl search__submit" tabindex="5">
        <i class="search__submit__icon mdi mdi-magnify"></i><span class="search__submit__text">Найти</span>
      </button>
      <button class="search__toggler" tabindex="4" title="Расширенный поиск" data-tooltip="simple">
        <i class="search__toggler__icon mdi mdi-tune"></i><span class="search__toggler__text">Параметры</span>
      </button><span class="search__reset ctrl hide" title="Очистить поле поиска" data-tooltip="simple"><span class="mdi mdi-close"></span></span>
      <div class="search__tag-counter hide">
        <span class="tag" data-tooltip="simple" title="Выбрано параметров расширенного поиска"><span class="tag__text">Выбрано</span><span class="tag__num">0</span></span>
      </div>
      <div class="search__container">
        <input type="text" id="search-input" class="search__string" placeholder="Искать: кодексы, аналитика, НПА, формы..." value="" tabindex="2">
      </div>
    </div>
    <div id="search_results" class="search-autocomplete hide hide">
      <div class="rubric-suggestion-container"></div>
    </div>
  </div>
